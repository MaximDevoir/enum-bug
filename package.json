{
  "name": "enum-bug",
  "version": "1.0.7",
  "description": "Enumerate bugged properties that will not enumerate in older browsers.",
  "main": "./dist/enum-bug.js",
  "license": "MIT",
  "author": {
    "name": "Maxim Devoir",
    "email": "maximdevoir@hotmail.com"
  },
  "scripts": {
    "build": "npm run build:source",
    "build:source": "cross-env NODE_ENV=production webpack-cli --progress --colors",
    "build:clean": "rm -rf ./dist/* && rm -rf ./docs/* && rm -rf ./coverage/*",
    "build:docs": "jsdoc -c .jsdoc.json",
    "coveralls": "./node_modules/.bin/nyc report --reporter=lcov --reporter=text ./node_modules/mocha/bin/_mocha --require @babel/register tests/*.spec.js",
    "lint": "eslint --ignore-path .eslintignore .",
    "prebuild": "npm run build:clean && npm run test",
    "pretest": "npm run lint",
    "start": "cross-env NODE_ENV=development webpack-cli --watch --progress --colors",
    "test": "mocha --require @babel/register tests/*.spec.js"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "@babel/register": "^7.4.4",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "babel-plugin-add-module-exports": "^1.0.2",
    "chai": "^4.2.0",
    "cross-env": "^5.2.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.13.0",
    "istanbul": "^0.4.5",
    "jsdoc": "^3.6.1",
    "minami": "^1.2.3",
    "mocha": "^6.1.4",
    "nyc": "^14.1.0",
    "webpack": "^4.30.0",
    "webpack-cli": "^3.3.2"
  }
}
