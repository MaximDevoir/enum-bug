{
  "name": "enum-bug",
  "version": "1.0.0",
  "description": "Enumerate bugged properties that will not enumerate in older browsers.",
  "main": "./dist/enumbug.js",
  "license": "MIT",
  "author": {
    "name": "Maxim Devoir",
    "email": "maximdevoir@hotmail.com"
  },
  "scripts": {
    "build": "npm run build:source",
    "build:source": "cross-env NODE_ENV=production webpack --progress --colors",
    "build:clean": "rm -rf ./dist/* && rm -rf ./docs/* && rm -rf ./coverage/*",
    "build:docs": "jsdoc -c .jsdoc.json",
    "coveralls": "./node_modules/.bin/babel-node ./node_modules/istanbul/lib/cli cover ./node_modules/mocha/bin/_mocha -- ./tests/*.spec.js",
    "lint": "eslint --ignore-path .eslintignore .",
    "prebuild": "npm run build:clean && npm run test",
    "postbuild": "npm run coveralls && npm run build:docs",
    "pretest": "npm run lint",
    "start": "cross-env NODE_ENV=development webpack --watch --progress --colors",
    "test": "mocha --compilers js:babel-register tests/*.spec.js"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.18.2",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.8",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-stage-0": "^6.16.0",
    "babel-register": "^6.18.0",
    "chai": "^3.5.0",
    "cross-env": "^3.1.3",
    "eslint": "^3.11.1",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-loader": "^1.6.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^2.0.1",
    "eslint-plugin-react": "^6.7.1",
    "istanbul": "^1.0.0-alpha.2",
    "jsdoc": "^3.4.3",
    "minami": "^1.1.1",
    "mocha": "^3.2.0",
    "webpack": "^1.13.3"
  }
}
